{
  "hosting": {
    "public": "dist/proyecto",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites":[
      {
        "source":"**",
        "destination":"/index.html"
    
    }
    ]
  },
  "headers": [
    {
        "source": "https://mi-porfolio-back-end.herokuapp.com/",
        "headers": [
            {
                "key": "Access-Control-Allow-Origin",
                "value": "https://mi-porfolio-back-end.herokuapp.com/"
            }
        ]
    }
]
}
